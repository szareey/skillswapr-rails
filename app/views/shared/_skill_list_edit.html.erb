<form method='post' action='/users/<%=@user.id%>/<%=type%>s/<%=skill_list_edit.id%>'> 
<input name="_method" type="hidden" value="delete">
<input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden"
<span class="label label-<%= format %>"><%= skill_list_edit.name.capitalize %></span>
<input type='hidden' value='<%= skill_list_edit.id %>' name='<%=type%>_id' >
<button type='submit'>&times;</button>
</form>